<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="189.0" prefWidth="224.0" spacing="3" styleClass="DarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.greenfox.memorycardgame.gui.MemoryGameController">
    <children>
        <Label prefHeight="38.0" prefWidth="55.0" text="Players:" />

        <HBox alignment="CENTER" spacing="10">
            <padding>

                <Insets>
                    <bottom>5</bottom>
                    <top>25</top>
                    <left>5</left>
                    <right>5</right>
                </Insets>

            </padding>
            <children>
                <Button fx:id="btn2" mnemonicParsing="false" onAction="#handleButtonAction"  text="Button" />
                <Button fx:id="restartGameButton" styleClass="button" text="new game" />
                <Button fx:id="chooseRootDirButton" styleClass="button" text="choose your pics" />
            </children>
        </HBox>

    </children>
</VBox>
